                    <section class="products-view products-view-grid">
                        <div class="row">
                             {{#each products}}
                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                <div class="product-box product_grid_2">
                                    <div class="product_mini_2 clearfix  ">
                                        <div class="img_product">
                                            <a href="/san-pham/{{this.slug}}" class="product-img" title="{{this.name}}">
                                                <img src="{{this.img}}" alt="{{this.name}}">
                                            </a>
                                        </div>
                                        <div class="product-info">
                                            <div class="infor_prd">
                                                <h3 class="name_product">
                                                    <a href="/san-pham/{{this.slug}}" title="{{this.name}}"
                                                        class="product-name">
                                                        <span class="name_product_other">{{this.name}}</span>
                                                    </a>
                                                </h3>
                                            </div>
                                            <div class="price_base price_base_other">
                                                <div class="price-box clearfix">
                                                    <div class="special-price">
                                                        <span class="price product-price">{{this.cost}}</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="summary_grid">
                                                <div class="rte description text2line">
                                                    {{this.name}} là sự kết hợp giữa nước dừa tươi và Espresso
